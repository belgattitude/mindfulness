{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "env.mjs",
    "next.config.mjs",
    "dist",
    "public"
  ],
  "scripts": {
    "dev": "next dev",
    "?dev-no-tls": "Disable node tls verifications in dev. So we can use self-signed certs",
    "dev-no-tls": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 next build --no-lint",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location .cache/web.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo .cache ./node_modules/.cache storybook-static",
    "codegen-schema": "dotenv-flow -- yarn graphql-codegen --config codegen.schema-ast.ts",
    "codegen": "dotenv-flow -- yarn graphql-codegen --config codegen.ts",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "deps:check": "npx npm-check-updates@latest --configFileName ncurc.yml",
    "deps:update": "npx npm-check-updates@latest --configFileName ncurc.yml -u",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "nuke:node_modules": "rimraf --glob '**/node_modules'"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.1.1",
    "@httpx/exception": "2.5.7",
    "@nextvalid/zod-request": "0.4.0",
    "@t3-oss/env-core": "0.7.1",
    "@t3-oss/env-nextjs": "0.7.1",
    "@tanstack/react-query": "5.14.6",
    "@vercel/og": "0.6.1",
    "clsx": "2.0.0",
    "date-fns": "3.0.6",
    "dayjs": "1.11.10",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "next": "14.0.4",
    "next-secure-headers": "2.2.0",
    "next-seo": "6.4.0",
    "nodemailer": "6.9.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "1.10.1",
    "react-markdown": "9.0.1",
    "remark": "15.0.1",
    "remark-gfm": "4.0.0",
    "remark-html": "16.0.1",
    "rooks": "7.14.1",
    "sharp": "0.33.1",
    "tailwind-merge": "2.2.0",
    "theme-ui": "0.16.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@belgattitude/eslint-config-bases": "4.8.0",
    "@emotion/babel-preset-css-prop": "11.11.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-react-query": "6.0.0",
    "@next/bundle-analyzer": "14.0.4",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/addon-links": "7.6.6",
    "@storybook/blocks": "7.6.6",
    "@storybook/nextjs": "7.6.6",
    "@storybook/react": "7.6.6",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/d": "1.0.4",
    "@types/node": "20.10.5",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "cross-env": "7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-tailwindcss": "3.13.0",
    "graphql-config": "5.0.3",
    "postcss": "8.4.32",
    "postcss-loader": "7.3.3",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "storybook": "7.6.6",
    "style-loader": "3.3.3",
    "tailwindcss": "3.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.1.0",
    "webpack": "5.89.0",
    "webpack-merge": "5.10.0"
  },
  "packageManager": "yarn@4.0.0-rc.50"
}
