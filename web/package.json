{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "env.mjs",
    "next.config.mjs",
    "dist",
    "public"
  ],
  "scripts": {
    "dev": "next dev",
    "?dev-no-tls": "Disable node tls verifications in dev. So we can use self-signed certs",
    "dev-no-tls": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 next build --no-lint",
    "typegen": "next typegen",
    "start": "next start",
    "lint": "next typegen && eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location .cache/web.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "next typegen && tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo .cache ./node_modules/.cache storybook-static",
    "codegen-schema": "dotenv-flow -- yarn graphql-codegen --config codegen.schema-ast.ts",
    "codegen": "dotenv-flow -- yarn graphql-codegen --config codegen.ts",
    "codegen-watch": "dotenv-flow -- yarn graphql-codegen --config codegen.ts --watch",
    "deps:check": "npx npm-check-updates@latest --configFileName .ncurc.yml",
    "deps:update": "npx npm-check-updates@latest --configFileName .ncurc.yml -u",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "nuke:node_modules": "rimraf --glob '**/node_modules'"
  },
  "dependencies": {
    "@httpx/assert": "0.12.4",
    "@httpx/exception": "3.1.8",
    "@httpx/plain-object": "2.1.7",
    "@t3-oss/env-core": "0.11.1",
    "@t3-oss/env-nextjs": "0.11.1",
    "@tanstack/react-query": "5.90.12",
    "@vercel/og": "0.8.5",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "dayjs": "1.11.19",
    "graphql": "16.12.0",
    "graphql-request": "7.3.5",
    "next": "16.0.7",
    "next-secure-headers": "2.2.0",
    "next-seo": "6.8.0",
    "nodemailer": "6.9.14",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-google-recaptcha-v3": "1.11.0",
    "react-markdown": "9.1.0",
    "react-twc": "1.5.1",
    "rehype-external-links": "3.0.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.1",
    "remark-html": "16.0.1",
    "rooks": "9.4.0",
    "sharp": "0.34.5",
    "tailwind-merge": "3.4.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@belgattitude/eslint-config-bases": "8.7.1",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-react-query": "6.1.1",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-themes": "8.2.9",
    "@storybook/addon-viewport": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/nextjs": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/test": "8.2.9",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@types/node": "22.5.1",
    "@types/nodemailer": "6.4.15",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "dotenv-flow-cli": "1.1.1",
    "eslint": "8.57.1",
    "eslint-config-next": "16.0.7",
    "graphql-config": "5.1.2",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "prettier": "3.3.3",
    "rimraf": "6.1.2",
    "storybook": "8.2.9",
    "style-loader": "4.0.0",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "5.4.2",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.0.5",
    "webpack": "5.103.0"
  },
  "packageManager": "yarn@4.12.0",
  "resolutions": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
