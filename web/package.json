{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "?dev-no-tls": "Disable node tls verifications in dev. So we can use self-signed certs",
    "dev-no-tls": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 next build --no-lint",
    "start": "next start",
    "lint": "TIMING=1 eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../.cache/web.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache storybook-static",
    "codegen-schema": "dotenv-flow -- yarn graphql-codegen --config codegen.schema-ast.ts",
    "codegen": "dotenv-flow -- yarn graphql-codegen --config codegen.ts",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "deps:check": "npx npm-check-updates@latest --configFileName ncurc.yml",
    "deps:update": "npx npm-check-updates@latest --configFileName ncurc.yml -u",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "nuke:node_modules": "rimraf --glob '**/node_modules'",
    "favpie": "npx favpie@latest ./public/logo/logo-sandrine-transparent.png ./public/favicon --app-name 'Sandrine Rauter' --short-name 'Sandrine Rauter'"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@headlessui/react": "1.7.15",
    "@heroicons/react": "2.0.18",
    "@httpx/exception": "1.7.2",
    "@nextvalid/zod-request": "0.4.0",
    "@tanstack/react-query": "4.29.12",
    "@vercel/og": "0.5.6",
    "clsx": "1.2.1",
    "date-fns": "2.30.0",
    "dayjs": "1.11.8",
    "graphql": "16.6.0",
    "graphql-request": "6.1.0",
    "next": "13.4.4",
    "next-secure-headers": "2.2.0",
    "next-seo": "6.0.0",
    "nodemailer": "6.9.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "1.10.1",
    "react-markdown": "8.0.7",
    "remark": "14.0.3",
    "remark-gfm": "3.0.1",
    "remark-html": "15.0.2",
    "rooks": "7.14.1",
    "sharp": "0.32.1",
    "tailwind-merge": "1.13.0",
    "theme-ui": "0.15.7",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@belgattitude/eslint-config-bases": "1.33.0",
    "@emotion/babel-preset-css-prop": "11.11.0",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.0",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@next/bundle-analyzer": "13.4.4",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.18",
    "@storybook/blocks": "7.0.18",
    "@storybook/nextjs": "7.0.18",
    "@storybook/react": "7.0.18",
    "@svgr/webpack": "8.0.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/d": "1.0.1",
    "@types/node": "20.2.5",
    "@types/nodemailer": "6.4.8",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "cross-env": "7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "graphql-config": "5.0.2",
    "postcss": "8.4.24",
    "postcss-loader": "^7.3.2",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "storybook": "7.0.18",
    "style-loader": "3.3.3",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.0",
    "webpack": "5.86.0",
    "webpack-merge": "5.9.0"
  },
  "packageManager": "yarn@4.0.0-rc.42"
}
