{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "env.mjs",
    "next.config.mjs",
    "dist",
    "public"
  ],
  "scripts": {
    "dev": "next dev",
    "?dev-no-tls": "Disable node tls verifications in dev. So we can use self-signed certs",
    "dev-no-tls": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 next build --no-lint",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location .cache/web.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo .cache ./node_modules/.cache storybook-static",
    "codegen-schema": "dotenv-flow -- yarn graphql-codegen --config codegen.schema-ast.ts",
    "codegen": "dotenv-flow -- yarn graphql-codegen --config codegen.ts",
    "codegen-watch": "dotenv-flow -- yarn graphql-codegen --config codegen.ts --watch",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "deps:check": "npx npm-check-updates@latest --configFileName .ncurc.yml",
    "deps:update": "npx npm-check-updates@latest --configFileName .ncurc.yml -u",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "nuke:node_modules": "rimraf --glob '**/node_modules'"
  },
  "dependencies": {
    "@httpx/assert": "0.12.4",
    "@httpx/exception": "3.0.9",
    "@httpx/plain-object": "1.1.1",
    "@t3-oss/env-core": "0.11.1",
    "@t3-oss/env-nextjs": "0.11.1",
    "@tanstack/react-query": "5.53.1",
    "@vercel/og": "0.6.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "dayjs": "1.11.13",
    "graphql": "16.9.0",
    "graphql-request": "7.1.0",
    "next": "14.2.7",
    "next-secure-headers": "2.2.0",
    "next-seo": "6.5.0",
    "nodemailer": "6.9.14",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-google-recaptcha-v3": "1.10.1",
    "react-markdown": "9.0.1",
    "react-twc": "1.4.2",
    "rehype-external-links": "3.0.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.0",
    "remark-html": "16.0.1",
    "rooks": "7.14.1",
    "sharp": "0.33.5",
    "tailwind-merge": "2.5.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@belgattitude/eslint-config-bases": "5.17.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-react-query": "6.1.0",
    "@next/bundle-analyzer": "14.2.7",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-themes": "8.2.9",
    "@storybook/addon-viewport": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/nextjs": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/test": "8.2.9",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/forms": "0.5.8",
    "@tailwindcss/typography": "0.5.15",
    "@types/node": "22.5.1",
    "@types/nodemailer": "6.4.15",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "cross-env": "7.0.3",
    "css-loader": "^7.1.2",
    "dotenv-flow-cli": "1.1.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.7",
    "eslint-plugin-tailwindcss": "3.17.4",
    "graphql-config": "5.1.2",
    "postcss": "8.4.41",
    "postcss-loader": "8.1.1",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "storybook": "8.2.9",
    "style-loader": "4.0.0",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.0.5",
    "webpack": "5.94.0"
  },
  "packageManager": "yarn@4.12.0"
}
